  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JohnFultonTW/sw_quiz/issues"
  },
  "homepage": "https://github.com/JohnFultonTW/sw_quiz#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "fetch-mock": "^5.0.3",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "lodash": "^4.13.1",
    "material-design-lite": "^1.1.3",
    "random-js": "^1.0.8",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "ava": {
    "files": [
      "./app/**/*.spec.js"
    ],
    "source": [
      "./app/**/*.js",
      "!./app/**/*.spec.js"
